<charts>
    <chart>
        <title>例子</title>
        <action>
            <operation>from</operation>
            <name>charts.csv</name>
        </action>

        <action>
            <operation>to</operation>
            <name>例子.jpg</name>
            <axis>
                <position>x</position>
                <series>
                    <name>date</name>
                    <column>axis</column>
                </series>
            </axis>
            <axis>
                <position>y1</position>

                <series>
                    <name>height</name>
                    <column>height</column>
                    <render>bar</render>
                    <applications>
                        <application>
                            <name>group</name>
                            <column>group</column>
                        </application>
                        <application>
                            <name>break</name>
                            <column>type</column>
                        </application>
                    </applications>
                </series>

            </axis>
            <axis>
                <applications>
                    <application>
                        <name>format</name>
                        <column>0.#%</column>
                    </application>
                </applications>
                <position>y2</position>
                <!--<range>-->
                    <!--<from>0</from>-->
                    <!--<to>100</to>-->
                <!--</range>-->
                <series>
                    <name>per</name>
                    <column>per</column>
                    <render>line</render>
                    <applications>
                        <application>
                            <name>format</name>
                            <column>0.00%</column>
                        </application>
                    </applications>
                </series>
                <!--<series>-->
                    <!--<name>weight</name>-->
                    <!--<column>weight</column>-->
                    <!--<render>line</render>-->
                <!--</series>-->
            </axis>
        </action>
    </chart>
</charts>